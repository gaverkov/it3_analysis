   "source": [
    "var('x')\n",
    "f(x) = x^2 - 5.0\n",
    "def newton(f,start,num_iter):\n",
    "    p = start\n",
    "    for i in range(num_iter):\n",
    "        p = p - f.diff()(p)^(-1)*f(p)\n",
    "    return p"
   ]
  },
